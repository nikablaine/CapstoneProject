<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp"
    android:id="@+id/constraintLayout3">

    <android.support.v7.widget.Toolbar
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:theme="?attr/actionBarTheme"
        android:minHeight="?attr/actionBarSize"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:elevation="@dimen/abc_text_size_title_material_toolbar"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout3"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout3"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout3"
        tools:layout_constraintRight_creator="0" />

    <fragment
        android:name="org.kraflapps.motiondroid.CameraFragment"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="80dp"
        android:id="@+id/fragment"
        tools:layout="@layout/fragment_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout3"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        android:layout_marginTop="24dp"
        tools:layout_constraintTop_creator="24dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout3"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintHorizontal_bias="1.0" />

    <android.support.design.widget.FloatingActionButton
        android:src="@drawable/abc_textfield_search_material"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        tools:layout_editor_absoluteX="328dp"
        tools:layout_editor_absoluteY="455dp"
        android:id="@+id/fab"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout3"
        android:layout_marginEnd="16dp"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout3"
        android:layout_marginBottom="16dp"
        tools:layout_constraintBottom_creator="0" />


</android.support.constraint.ConstraintLayout>